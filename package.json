{
  "name": "vn-payments",
  "version": "1.0.0",
  "description": "Helpers for various Payment Gateway in Vietnam",
  "repository": "https://github.com/naustudio/vn-payments",
  "author": "Nau Studio <contact@naustud.io>",
  "contributors": [
    "Thanh Tran <thanh@naustud.io>"
  ],
  "license": "Apache-2.0",
  "main": "dist/index.js",
  "engines": {
    "node": ">=6.9.1",
    "npm": ">=3.10.0"
  },
  "scripts": {
    "__prestart": "npm run -s build",
    "start": "nodemon example/index.js --exec \"node -r dotenv/config\"",
    "clean": "rimraf dist",
    "build": "npm run lint && jest --config test/jest.config.js --coverage && npm run clean && mkdir -p dist && babel src -s -D -d dist && codecov",
    "test": "jest --watch",
    "lint": "esw src test example"
  },
  "keywords": [
    "express",
    "babel",
    "es6",
    "es2015",
    "es2016",
    "es2017",
    "jest",
    "eslint"
  ],
  "dependencies": {
    "simpl-schema": "^1.4.1"
  },
  "@std/esm": "cjs",
  "devDependencies": {
    "@std/esm": "^0.19.1",
    "babel-cli": "^6.26.0",
    "babel-eslint": "^8.0.3",
    "babel-jest": "^21.2.0",
    "babel-preset-env": "^1.6.1",
    "babel-register": "^6.26.0",
    "body-parser": "^1.18.2",
    "codecov": "^3.0.0",
    "dotenv": "^4.0.0",
    "eslint": "^4.12.1",
    "eslint-config-nau": "^13.1.0",
    "eslint-plugin-import": "^2.8.0",
    "eslint-plugin-jest": "^21.3.2",
    "eslint-watch": "^3.1.3",
    "express": "^4.16.2",
    "express-handlebars": "^3.0.0",
    "jest": "^21.2.1",
    "morgan": "^1.9.0",
    "nodemon": "^1.12.1",
    "rimraf": "^2.6.2",
    "supertest": "^3.0.0"
  },
  "babel": {
    "presets": [
      [
        "env",
        {
          "targets": {
            "node": "current"
          }
        }
      ]
    ]
  },
  "jest": {
    "testEnvironment": "node"
  }
}
